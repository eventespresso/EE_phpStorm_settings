<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="c4d7e7" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="c4d7e7" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingLevelCount" value="10" />
  </component>
</application>